{
  "expo": {
    "name": "MAPTApp",
    "slug": "MAPTApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show your position on the map"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img/logo2.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
